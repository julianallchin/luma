{
	"name": "luma",
	"private": true,
	"license": "PolyForm-Shield-1.0.0",
	"version": "0.2.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"tauri": "tauri",
		"tauri:perf": "tauri dev -- --profile perf",
		"lint": "biome ci . && cargo clippy --manifest-path src-tauri/Cargo.toml --all-targets",
		"typecheck": "tsc --noEmit && cargo check --manifest-path src-tauri/Cargo.toml --all-targets",
		"format": "biome check --write . && cargo fmt --manifest-path src-tauri/Cargo.toml",
		"biome:check": "biome check .",
		"biome:format": "biome format --write .",
		"prepare": "husky"
	},
	"dependencies": {
		"@radix-ui/react-checkbox": "^1.3.3",
		"@radix-ui/react-context-menu": "^2.2.16",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-hover-card": "^1.1.15",
		"@radix-ui/react-label": "^2.1.8",
		"@radix-ui/react-popover": "^1.1.15",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-slot": "^1.2.4",
		"@react-three/drei": "^10.7.7",
		"@react-three/fiber": "^9.4.0",
		"@react-three/postprocessing": "^3.0.4",
		"@supabase/supabase-js": "^2.89.0",
		"@tauri-apps/api": "^2.10.1",
		"@tauri-apps/plugin-dialog": "^2.6.0",
		"@tauri-apps/plugin-opener": "^2",
		"@tauri-apps/plugin-sql": "^2.3.1",
		"@types/three": "^0.181.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"color": "^5.0.3",
		"input-otp": "^1.4.2",
		"lucide-react": "^0.553.0",
		"next-themes": "^0.4.6",
		"postprocessing": "^6.38.0",
		"radix-ui": "^1.4.3",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router-dom": "^7.13.0",
		"reactflow": "^11.11.4",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.3.1",
		"three": "^0.181.2",
		"zustand": "^5.0.8"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,css}": "biome check --write --no-errors-on-unmatched",
		"*.rs": "cargo fmt --manifest-path src-tauri/Cargo.toml --"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.5",
		"@tailwindcss/vite": "^4.1.16",
		"@tauri-apps/cli": "^2",
		"@types/color": "^4.2.0",
		"@types/node": "^24.9.2",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@vitejs/plugin-react": "^4.6.0",
		"daisyui": "^5.3.10",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"tailwindcss": "^4.1.16",
		"tw-animate-css": "^1.4.0",
		"typescript": "~5.8.3",
		"vite": "^7.0.4"
	}
}
